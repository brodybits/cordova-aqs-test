<plugin id="cordova-aqs-android" version="0.0.1-dev">
  <platform name="android">
    <!-- THANKS: cordova-plugin-whitelist -->
    <config-file target="res/xml/config.xml" parent="/*">
      <feature name="AQS">
        <param name="android-package" value="me.brodybits.AQS" />
        <param name="onload" value="true" />
      </feature>
    </config-file>

    <source-file src="src/android/me/brodybits/AQS.java" target-dir="src/me/brodybits" />
  </platform>
</plugin>
